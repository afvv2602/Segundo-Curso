<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--  Color  -->
    <item>
        <shape android:shape="oval">
            <gradient
                android:startColor="#bbceda"
                android:endColor="#e6e8ec"
                android:gradientRadius="100%"
                android:centerX="50%"
                android:centerY="50%"
                android:type="radial" />
            <stroke
                android:width="5dp"
                android:color="#EBE9ED" />
        </shape>
    </item>
</selector>
